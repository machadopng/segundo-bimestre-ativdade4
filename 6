programa
{
	
	funcao inicio()
	{
          inteiro n1, n2, i
          escreva("Me dê dois números inteiros.\n")
          leia(n1, n2)
          se(n1 < n2)
          {
          	para(i = n1; i <= n2; i++)
          	{
          
          	escreva(i, " ")
               }
          }    
          
          se(n1 > n2)
          {
          	para(i = n2; i <= n1; i++)
          	{
          
          	escreva(i, " ")
               }
          }    
          
	}
}
